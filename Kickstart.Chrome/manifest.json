{
  "name": "Kickstart",
  "description": "Invokes all website pages found in the site map",
  "version": "0.0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Kickstart this Website"
  },
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },
  "manifest_version": 2
}
